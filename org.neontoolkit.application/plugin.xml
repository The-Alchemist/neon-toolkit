<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         name="NeOn Toolkit"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.neontoolkit.application.intro.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="org.neontoolkit.application.intro">
      </intro>
      <introProductBinding
            introId="org.neontoolkit.application.intro"
            productId="org.neontoolkit.application.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            introId="org.neontoolkit.application.intro"
            content="introContent.xml"
            id="org.neontoolkit.application.configId">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx">
            </implementation>
         </presentation>
      </config>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            content="ext.xml"
            configId="org.neontoolkit.application.configId">
      </configExtension>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.neontoolkit.application.application"
            name="NeOn Toolkit">
         <property
               name="appName"
               value="NeOn Toolkit">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="icons/neon_large.png">
         </property>
         <property
               name="aboutText"
               value="NeOn Toolkit 2.3.3&#x0D;&#x0A;&#x0D;&#x0A;Distributed by the NeOn Technologies Foundation Ltd.&#x0D;&#x0A;http://www.neon-toolkit.org/&#x0D;&#x0A;&#x0D;&#x0A;The NeOn Toolkit bundle is licensed under the Eclipse Public License &#x0D;&#x0A;(EPL) version 1.0. Individual plugins are also usually provided under &#x0D;&#x0A;EPL.  Please check the license agreements associated with individual &#x0D;&#x0A;plugins by going to  Help &gt; Software Updates &gt; Manage Configuration&#x0D;&#x0A;&#x0D;&#x0A;This product includes software developed by the Apache Software Foundation &#x0D;&#x0A;http://www.apache.org/">
         </property>
         <property
               name="windowImages"
               value="icons/neon_16x16.png,icons/neon_32x32.png,icons/neon_48x48.bmp,icons/neon_64x64.png,icons/neon_128x128.png">
         </property>
      </product>
   </extension>

</plugin>
